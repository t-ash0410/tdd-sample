{
    "name": "tdd-sample-backend",
    "dockerComposeFile": [
        "../../docker-compose.yml"
    ],
    "service": "backend-builder",
    "workspaceFolder": "/backend",
    "extensions": [
        "ms-azuretools.vscode-docker",
        "golang.go",
        "dunstontc.vscode-go-syntax",
        "be5invis.toml",
        "zxh404.vscode-proto3"
    ],
    "settings": {
        "editor.tabSize": 2,
        "files.insertFinalNewline": true,
        "files.trimFinalNewlines": true,
    },
    "shutdownAction": "none"
}